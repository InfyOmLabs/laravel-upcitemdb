{
    "name": "infyomlabs/laravel-upcitemdb",
    "description": "Laravel Wrapper for UPCItemDB.",
    "type": "library",
    "keywords": [
        "laravel",
        "upcitemdb",
        "infyom",
        "upc lookup",
        "upc",
        "ean",
        "isbn"
    ],
    "authors": [
        {
            "name": "Mitul Golakiya",
            "email": "me@mitul.me"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "~6.0|^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.5"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\UPCItemDB\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\InfyOm\\UPCItemDB\\UPCItemDBServiceProvider"
            ],
            "aliases": {
                "UPCItemDB": "InfyOm\\UPCItemDB\\UPCItemDB"
            }
        }
    },
    "funding": [
        {
            "type": "opencollective",
            "url": "https://opencollective.com/infyomlabs"
        }
    ]
}
